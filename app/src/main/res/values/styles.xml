<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/reach_color</item>
        <item name="colorPrimaryDark">@color/reach_dark</item>
        <item name="colorAccent">@color/reach_blue</item>
        <item name="searchViewStyle">@style/MySearchViewStyle</item>
        <item name="windowActionBarOverlay">true</item>
    </style>
    <style name="MySearchViewStyle" parent="Widget.AppCompat.SearchView">
        <item name="searchIcon">@drawable/ic_search_white</item>
    </style>
    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_menu_bg</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>
