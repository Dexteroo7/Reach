<resources>

    <style name="AppThemeTry" parent="BaseAppThemeTry">

        </style>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/reach_color</item>
        <item name="colorPrimaryDark">@color/reach_dark</item>
        <item name="colorAccent">@color/reach_color</item>
        <item name="android:fitsSystemWindows">true</item>
        <!--<item name="actionOverflowMenuStyle">@style/OverflowMenu</item>-->

    </style>

    <style name="BaseAppThemeTry" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/reach_color</item>
        <item name="colorPrimaryDark">@color/reach_dark</item>
        <item name="colorAccent">@color/reach_color</item>
        <!--<item name="actionOverflowMenuStyle">@style/OverflowMenu</item>-->

    </style>

    <style name="SplashTheme" parent="AppTheme">
        <item name="android:windowBackground">@drawable/splash_bg</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="OverflowMenu" parent="Widget.AppCompat.PopupMenu.Overflow">
        <!-- Required for pre-Lollipop. -->
        <item name="overlapAnchor">false</item>

        <!-- Required for Lollipop. -->
        <item name="android:overlapAnchor">false</item>
    </style>

</resources>
